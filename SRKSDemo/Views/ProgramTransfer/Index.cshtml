<script>
    function validate() {
        var filename = $("#filename").html();
        var data = $("#content-target").val();
        var arry1 = filename.split('.');
        filename = arry1[0];
        var arry2 = data.split('\n');
        var arry3 = arry2[1].split('(');
        if (filename == arry3[0]) {
            $("#fileError").hide();
            $("#content-target").show();
        } else {
