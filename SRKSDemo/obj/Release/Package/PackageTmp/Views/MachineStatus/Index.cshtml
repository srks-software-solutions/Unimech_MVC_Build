                                    @for (var j = 0; j < count1; j++)
                                    {
                                        if (model1[j].MachineID == row1.MachineID && model1[j].tblmachinedetail.tblcell.CelldisplayName == shopname)
                                        {
                                            var Width = model1[j].DurationInSec * 0.069;
                                            var Dura = model1[j].DurationInSec;
                                            if (model1[j].ColorCode == "GREEN")
                                            {
                                                <span style="background-color: green; color: green; float: left; height: 27px; width:@Width%; padding-bottom: 10px" data-toggle="tooltip" title="@("Operating " + Dura + " min")">.</span>
                                            }
                                            else if (model1[j].ColorCode == "YELLOW")
                                            {
                                                if (model1[j].LossCodeID != null)
                                                {
                                                    var losscodedesc = model1[j].tbllossescode.LossCodeDesc;
                                                    <span style="background-color: yellow; color: yellow; float: left; height: 27px; width: @Width%; padding-bottom: 10px" data-toggle="tooltip" title="@("Loss " + Dura + " min -- " + losscodedesc)">.</span>
                                                }
                                                else
                                                {
                                                    <span style="background-color: yellow; color: yellow; float: left; height: 27px; width: @Width%; padding-bottom: 10px" data-toggle="tooltip" title="@("Loss " + Dura + " min")">.</span>
                                                }
                                            }
                                            else if (model1[j].ColorCode == "RED")
                                            {
                                                if (model1[j].BreakdownID != null)
                                                {
                                                    var BDDesc = model1[j].tbllossescode.LossCodeDesc;
                                                    <span style="background-color: red; color: red; float: left; height: 27px; width: @Width%; padding-bottom: 10px" data-toggle="tooltip" title="@("Breakdown " + Dura + " min -- " + BDDesc)">.</span>
                                                }
