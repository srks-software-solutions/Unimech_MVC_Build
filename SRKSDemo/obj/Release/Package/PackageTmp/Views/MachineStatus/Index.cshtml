                            {
                                var showData = 0;
                                var count1 = Model.Where(l => l.MachineID == row1.MachineID).ToList().Count;
                                var model1 = Model.Where(l => l.MachineID == row1.MachineID).OrderBy(l => l.tblmachinedetail.CellOrderNo).ToList();
                                for (var ja = 0; ja < count1; ja++)
