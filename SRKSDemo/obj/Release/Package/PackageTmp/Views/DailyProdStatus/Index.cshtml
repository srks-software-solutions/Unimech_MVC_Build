    }

    div.childtext {
        width: auto;
        height: 90%;
        float: right;
        -webkit-text-size-adjust: 100%;
        font-size: 1vw;
        color: black;
        display: inline;
        text-align: left;
        /*margin-right: 1vw;
        border-radius: 10px;*/
