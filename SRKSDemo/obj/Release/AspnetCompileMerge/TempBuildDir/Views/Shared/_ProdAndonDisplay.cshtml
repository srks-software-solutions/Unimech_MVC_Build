    <script>
    $(document).ready(function () {
    var myInterval = false;
    myInterval = setInterval(function () {
        var iScroll = $(window).scrollTop();
        if (iScroll + $(window).height() == $(document).height()) {
            $('html, body').animate({
                scrollTop: 0
            }, 1000);
        } else {
            iScroll = iScroll + 550;
            $('html, body').animate({
                scrollTop: iScroll
            }, 2000);
        }
    }, 12000);
});
    </script>
